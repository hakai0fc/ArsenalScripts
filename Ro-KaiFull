--Load
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/hakaiofc/Base/main/HUBORION')))()

--Main Script
local Window = OrionLib:MakeWindow({Name = "Hakai HubPriv", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest", IntroEnabled = false})

--ANTI AFK
pcall(function()
    local VirtualUser = game:service'VirtualUser'
    game:service'Players'.LocalPlayer.Idled:connect(function()
        VirtualUser:CaptureController()
        VirtualUser:ClickButton2(Vector2.new())
    end)
end)

--GLOBALS
local LP = game.Players.LocalPlayer
local ReplicatedStorage = game:GetService("ReplicatedStorage")

--Valor
_G.AutoStrength = false  
function AutoStrength()
    while _G.AutoStrength do
        local ReplicatedStorage = game:GetService("ReplicatedStorage")
        local damageRemote = ReplicatedStorage:WaitForChild("DamageRemote")
        local punchingBagHumanoid = workspace:WaitForChild("PunchingBag"):WaitForChild("PunchingBag"):WaitForChild("Humanoid")
        if punchingBagHumanoid then
            for i = 1, 70 do
                damageRemote:FireServer(punchingBagHumanoid, 15, false)
            end
        else
            warn("PunchingBag Humanoid não encontrado!")
            break
        end
        wait(0.000000000000000000000000000000001) 
    end
end
--Valor
_G.AutoStrength2 = false  
function AutoStrength2()
    while _G.AutoStrength2 do
        local ReplicatedStorage = game:GetService("ReplicatedStorage")
        local damageRemote = ReplicatedStorage:WaitForChild("DamageRemote")
        local punchingBagHumanoid = workspace:WaitForChild("PunchingBag"):WaitForChild("PunchingBag"):WaitForChild("Humanoid")
        if punchingBagHumanoid then
            for i = 1, 70 do
                damageRemote:FireServer(punchingBagHumanoid, 15, false)  
                damageRemote:FireServer(punchingBagHumanoid, 15, false) 
                damageRemote:FireServer(punchingBagHumanoid, 15, false)  
            end
        else
            warn("PunchingBag Humanoid não encontrado!")
            break
        end
        wait(0.000000000000000000000000000000001) 
    end
end

--Main
local FarmTab1 = Window:MakeTab({
    Name = "FarmStrength",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})
local Section = FarmTab1:AddSection({
    Name = "AutoFarm"
})
FarmTab1:AddToggle({
    Name = "Auto-Strength",
    Default = false,
    Callback = function(Value)
        _G.AutoStrength = Value
        if Value then
            AutoStrength()  
        end
    end
})

FarmTab1:AddToggle({
    Name = "Auto-Strength2",
    Default = false,
    Callback = function(Value)
        _G.AutoStrength2 = Value
        if Value then
            AutoStrength2()  
        end
    end
})
