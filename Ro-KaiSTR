--Load
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/hakaiofc/Base/main/HUBORION')))()

--Main Script
local Window = OrionLib:MakeWindow({Name = "Hakai HubPriv", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest", IntroEnabled = false})

--Valor
_G.AutoHealth = false  
function AutoHealth()
    while _G.AutoHealth do
        local ReplicatedStorage = game:GetService("ReplicatedStorage")
        local damageRemote = ReplicatedStorage:WaitForChild("DamageRemote")
        local punchingBagHumanoid = workspace:WaitForChild("PunchingBag"):WaitForChild("PunchingBag"):WaitForChild("Humanoid")
        if punchingBagHumanoid then
            for i = 1, 522 do
                damageRemote:FireServer(punchingBagHumanoid, 15, false)
            end
        else
            warn("PunchingBag Humanoid não encontrado!")
            break
        end
        wait(0.000000000000000000000000000000001) 
    end
end

--Valor
_G.AutoVida = false

function AutoVida()
    while _G.AutoVida do
        local ReplicatedStorage = game:GetService("ReplicatedStorage")
        local damageRemote = ReplicatedStorage:WaitForChild("DamageRemote")
        local punchingBagHumanoid = workspace:WaitForChild("PunchingBag"):WaitForChild("PunchingBag"):WaitForChild("Humanoid")
        if punchingBagHumanoid then
            for i = 1, 522 do
                damageRemote:FireServer(punchingBagHumanoid, 15, false)
            end
        else
            warn("PunchingBag Humanoid não encontrado!")
            break
        end
        wait(0.000000000000000000000001)
    end
end

--Valor
_G.AutoVD = false

function AutoVD()
    while _G.AutoVD do
        local ReplicatedStorage = game:GetService("ReplicatedStorage")
        local damageRemote = ReplicatedStorage:WaitForChild("DamageRemote")
        local punchingBagHumanoid = workspace:WaitForChild("PunchingBag"):WaitForChild("PunchingBag"):WaitForChild("Humanoid")
        if punchingBagHumanoid then
            for i = 1, 522 do
                damageRemote:FireServer(punchingBagHumanoid, 15, false)
            end
        else
            warn("PunchingBag Humanoid não encontrado!")
            break
        end
        wait(0.000000000000000000000001)
    end
end

--Main
local FarmTab = Window:MakeTab({
    Name = "Main",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})
local Section = FarmTab:AddSection({
    Name = "AutoFarm"
})
FarmTab:AddToggle({
    Name = "Auto-Strength",
    Default = false,
    Callback = function(Value)
        _G.AutoHealth = Value
        if Value then
            AutoHealth()  
        end
    end
})

--Main
local AutoTab = Window:MakeTab({
    Name = "Main2",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})
local Section = AutoTab:AddSection({
    Name = "AutoFarm2"
})
AutoTab:AddToggle({
    Name = "Auto-Strength2",
    Default = false,
    Callback = function(Value)
        _G.AutoVida = Value
        if Value then
            AutoVida()
        end
    end
})

--Main
local Vida3Tab = Window:MakeTab({
    Name = "Main3",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})
local Section = Vida3Tab:AddSection({
    Name = "AutoFarm3"
